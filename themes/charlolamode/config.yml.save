params:
    author: Pranav MR


    defaultTheme: auto
    # disableThemeToggle: true
    ShowShareButtons: true
    ShowReadingTime: true
    displayFullLangName: true
    ShowBreadCrumbs: true
    ShowCodeCopyButtons: true
    ShowToc: true

    profileMode:
        # Disable to populate home page with 'mainSections' content
        enabled: true
        title: Pranav MR
        description: |

          Hello, I am Pranav.
           I am a person interested in cybersecurity and blue teaming. 
        

          I am especially interested in malware analysis and reverse engineering and low level stuff.
        imageUrl: "/images/profile.png"
        imageTitle: my image
        # imageWidth: 120
        # imageHeight: 120
        buttons:
            - name: Don't click me
              url: https://www.youtube.com/@pranavtechy
    mainSections:
      - articles

    socialIcons:
        #- name: buymeacoffee
        #  url: "https://ko-fi.com/heycharlola"
        - name: github
          url: "https://github.com/pranavmr009"
        - name: linkedin
          url: "https://www.linkedin.com/in/pranavmr009"

      # add more social icons. find more in svg.html file
      # - name: youtube
      #   url: yoururl
      # - name: twitter
      #   url: yoururl
      # - name: facebook
      #   url: yoururl


    # change favicon in images folder (icon for web tab)
    assets:
          #favicon: "/images/favicon.ico"
          favicon: "/images/favicon.ico"
          favicon16x16: "/images/favicon-16x16.png"
          favicon32x32: "/images/favicon-32x32.png"
          apple_touch_icon: "/images/apple-touch-icon.png"
          safari_pinned_tab: "/images/apple-touch-icon.png"



enableInlineShortcodes: true
enableRobotsTXT: true
enableEmoji: true
pygmentsUseClasses: true

