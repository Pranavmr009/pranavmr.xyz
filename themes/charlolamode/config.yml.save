baseURL: https://pranavmr.xyz
title: pranavmrxyz
paginate: 5
theme: charlolamode



menu:
    main:
        - name: About
          url: /about/
          weight: 30
        - name: Articles
          url: /articles/
          weight: 20
        - name: Projects
          url: /projects/
          weight: 15

     # - name: Categories
        #   url: /categories/
